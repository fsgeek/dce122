    moveto
    0.0 ht rlineto
    wd 0.0 rlineto
    0.0 ht neg rlineto
    wd neg 0.0 rlineto
  closepath fill
  grestore
  wd 0.0 rmoveto
  currentpoint /Ypos xdef /Xpos xdef
} bdef

