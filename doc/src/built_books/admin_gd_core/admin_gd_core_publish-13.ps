% Exit page (temporarily) to add fonts/characters.
/XP {
  % Save current point information so it can be reset later. 
  /Xpos where {pop Xpos} {0} ifelse
  /Ypos where {pop Ypos} {0} ifelse
  SetCurrentPoint
  RestoreDviLaserPage
  moveto
  /Ypos xdef
  /Xpos xdef
} bdef
 
% Resume page
/RP {
