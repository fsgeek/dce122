% Begin document
/BeginDviLaserDoc {
  0 0 moveto
  vmstatus pop pop 0 eq 
    { $DviLaser begin }
    { /DviLaserJob save def
      $DviLaser begin
      /Xpos 0.0 def
      /Ypos 0.0 def
      /InitialMatrix matrix currentmatrix def }
    ifelse
} bind def
 
% End document
/EndDviLaserDoc {
  vmstatus pop pop 0 eq 
    { end }
    { DviLaserFonts restore
      end
      DviLaserJob restore }
    ifelse
} bind def
 
