    % % (disabled currently...)
    % GRfreq 0 ne
    %   {currentscreen
    %   3 -1 roll pop GRfreq 3 1 roll
    %   setscreen}
    % if
    % set the gray level
    GRgraylev setgray
    % draw and fill the path
    GRulx GRuly moveto
    GRlrx GRuly lineto
    GRlrx GRlry lineto
    GRulx GRlry lineto
    closepath
    fill
  grestore
  } bdef


