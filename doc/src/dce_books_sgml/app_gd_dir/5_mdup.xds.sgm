<Entry rowsep="1">0&ndash;2<?sml-size -2><Superscript>4</Superscript><?sml-size +2></Entry>
<Entry rowsep="1">&mdash;</Entry>
</Row>


<Row>
<Entry rowsep="1"><Literal>MH_T_TERMINAL_ IDENTIFIER</Literal></Entry>
<Entry rowsep="1">String<Literal>(OM_S_ PRINTABLE_ STRING)</Literal></Entry>
<Entry rowsep="1">1&ndash;24</Entry>
<Entry rowsep="1">0 or 1</Entry>
<Entry rowsep="1">&mdash;</Entry>
</Row>

<Row>
<Entry rowsep="1"><Literal>MH_T_TERMINAL_ TYPE</Literal></Entry>
<Entry rowsep="1">Enum<Literal>(MH_Terminal_ Type)</Literal></Entry>
<Entry rowsep="1">&mdash;</Entry>
<Entry rowsep="1">0 or 1</Entry>
<Entry rowsep="1">1988</Entry>
</Row>

<Row>
<Entry rowsep="1"><Literal>MH_T_X121_ ADDRESS</Literal></Entry>
<Entry rowsep="1">String<Literal>(OM_S_ NUMERIC_ STRING)</Literal></Entry>
<Entry rowsep="1">1&ndash;15</Entry>
<Entry rowsep="1">0 or 1</Entry>
<Entry rowsep="1">&mdash;</Entry>
</Row>

