<Entry rowsep="1">Value obtained from the cache or absent</Entry>
</Row>
<Row>
<Entry rowsep="1"><Literal>DS_DSA_NAME</Literal></Entry>
<Entry rowsep="1">Value obtained from the cache or absent</Entry>
</Row>
<Row>
<Entry rowsep="1"><Literal>DS_FILE_DESCRIPTOR</Literal></Entry>
<Entry rowsep="1"><Literal>DS_NO_VALID_FILE_DESCRIPTOR</Literal></Entry>
</Row>
<Row>
<Entry rowsep="1"><Literal>DSX_DIR_ID</Literal></Entry>
<Entry rowsep="1">1</Entry>
</Row>
<Row>
<Entry rowsep="1"><Literal>DSX_AUTH_MECHANISM</Literal></Entry>
<Entry rowsep="1">Absent</Entry>
</Row>
<Row>
