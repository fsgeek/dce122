<RefSect1>
<Title>Description</Title>
<Para>The <Filename>xdsgds.h</Filename> header declares the object identifiers of directory
attribute types and directory object classes supported by the GDS
package.  It also defines OM classes used to represent the values
of the attribute types.
<IndexTerm Id="DCEADR.MAN375.indx.1">
<Primary>GDS package</Primary>
</IndexTerm></Para>
<Para>All application programs that include this header must first include
the <Filename>xom.h</Filename> object management header and the <Filename>xds.h</Filename> header.
<?sml-space .5><?sml-point-size 11>
<!--no-op:  12-->
</Para>
<InformalExample>
<Para><ProgramListing>#ifndef XDSGDS_HEADER
#define XDSGDS_HEADER


/* GDS package object identifier */
/* iso(1) identified-organization(3) icd-ecma(0012) 
   member-company(2) siemens-units(1107) sni(1) directory(3) 
   xds-api(100)gdsp(0) */

#define OMP_O_DSX_GDS_PKG   &bsol;
"&bsol;x2B&bsol;x0C&bsol;x02&bsol;x88&bsol;x53&bsol;x01&bsol;x03&bsol;x64&bsol;x00"

/*Intermediate object identifier macros */

/* iso(1) identified-organization(3) icd-ecma(0012) 
   member-company(2) siemens-units(1107) sni(1) directory(3) 
   attribute-type(4) ...*/

#define dsP_GDSattributeType(X) &bsol;
("&bsol;x2B&bsol;x0C&bsol;x02&bsol;x88&bsol;x53&bsol;x01&bsol;x03&bsol;x04" #X)

/* iso(1) identified-organization(3) icd-ecma(0012)
   member-company(2) siemens-units(1107) sni(1) directory(3)
   object-class(6) ...*/

#define dsP_GDSobjectClass(X) &bsol;
("&bsol;x2B&bsol;x0C&bsol;x02&bsol;x88&bsol;x53&bsol;x01&bsol;x03&bsol;x06" #X)

#define dsP_gdsp_c(X)    OMP_O_DSX_GDS_PKG #X

<?sml-break><?sml-need 6>/* OM class names (prefixed by DSX_C_)
   Directory attribute types (prefixed by DSX_A_)
   Directory object classes (prefixed by DSX_O_)
*/


/* Directory attribute types */

#define OMP_O_DSX_A_MASTER_KNOWLEDGE dsP_GDSattributeType(&bsol;x00)
#define OMP_O_DSX_A_ACL              dsP_GDSattributeType(&bsol;x01)
#define OMP_O_DSX_A_TIME_STAMP       dsP_GDSattributeType(&bsol;x02)
#define OMP_O_DSX_A_SHADOWED_BY      dsP_GDSattributeType(&bsol;x03)
#define OMP_O_DSX_A_SRT              dsP_GDSattributeType(&bsol;x04)
#define OMP_O_DSX_A_OCT              dsP_GDSattributeType(&bsol;x05)
#define OMP_O_DSX_A_AT               dsP_GDSattributeType(&bsol;x06)
#define OMP_O_DSX_A_DEFAULT_DSA      dsP_GDSattributeType(&bsol;x08)
#define OMP_O_DSX_A_LOCAL_DSA        dsP_GDSattributeType(&bsol;x09)
#define OMP_O_DSX_A_CLIENT           dsP_GDSattributeType(&bsol;x0A)
#define OMP_O_DSX_A_DNLIST           dsP_GDSattributeType(&bsol;x0B)
#define OMP_O_DSX_A_SHADOWING_JOB    dsP_GDSattributeType(&bsol;x0C)
#define OMP_O_DSX_A_CDS_CELL         dsP_GDSattributeType(&bsol;x0D)
#define OMP_O_DSX_A_CDS_REPLICA      dsP_GDSattributeType(&bsol;x0E)


/* Directory object classes */

#define OMP_O_DSX_O_SCHEMA           dsP_GDSobjectClass(&bsol;x00)


<IndexTerm Id="DCEADR.MAN375.indx.2">
<Primary>OM</Primary>
<Secondary>class names</Secondary>
</IndexTerm>/* OM class names */

#define OMP_O_DSX_C_GDS_SESSION         dsP_gdsp_c(&bsol;x00)
#define OMP_O_DSX_C_GDS_CONTEXT         dsP_gdsp_c(&bsol;x01)
#define OMP_O_DSX_C_GDS_ACL             dsP_gdsp_c(&bsol;x02)
#define OMP_O_DSX_C_GDS_ACL_ITEM        dsP_gdsp_c(&bsol;x03)


/* OM attribute names */
<IndexTerm Id="DCEADR.MAN375.indx.3">
<Primary>OM</Primary>
<Secondary>attribute names</Secondary>
</IndexTerm>
#define DSX_PASSWORD                  ((OM_type) 850)
#define DSX_DIR_ID                    ((OM_type) 851)
#define DSX_DUAFIRST                  ((OM_type) 852)
#define DSX_DONT_STORE                ((OM_type) 853)
#define DSX_NORMAL_CLASS              ((OM_type) 854)
#define DSX_PRIV_CLASS                ((OM_type) 855)
#define DSX_RESIDENT_CLASS            ((OM_type) 856)
#define DSX_USEDSA                    ((OM_type) 857)
#define DSX_DUA_CACHE                 ((OM_type) 858)
#define DSX_MODIFY_PUBLIC             ((OM_type) 859)
#define DSX_READ_STANDARD             ((OM_type) 860)
#define DSX_MODIFY_STANDARD           ((OM_type) 861)
#define DSX_READ_SENSITIVE            ((OM_type) 862)
#define DSX_MODIFY_SENSITIVE          ((OM_type) 863)
#define DSX_INTERPRETATION            ((OM_type) 864)
#define DSX_USER                      ((OM_type) 865)
#define DSX_PREFER_ADM_FUNCS          ((OM_type) 866)
#define DSX_AUTH_MECHANISM            ((OM_type) 867)
#define DSX_AUTH_INFO                 ((OM_type) 868) /* future use */
#define DSX_SIGN_MECHANISM            ((OM_type) 869) /* future use */
#define DSX_PROT_REQUEST              ((OM_type) 870) /* future use */


/* DSX_Interpretation */

enum DSX_Interpretation {
        DSX_SINGLE_OBJECT    = 0,
        DSX_ROOT_OF_SUBTREE  = 1
};


enum DSX_Auth_Mechanism {
        DSX_DEFAULT          = 1,
        DSX_SIMPLE           = 2,
        DSX_SIMPLE_PROT1     = 3,
        DSX_SIMPLE_PROT2     = 4,
        DSX_DCE_AUTH         = 5,
        DSX_STRONG           = 6
};


enum DSX_Prot_Request {
        DSX_NONE             = 0,
        DSX_SIGNED           = 1
};


/* upper bound on string lengths*/

#define DSX_VL_PASSWORD         ((OM_value_length) 16)

#endif  /* XDSGDS_HEADER */
</ProgramListing></Para>
</InformalExample>
